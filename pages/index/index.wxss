/**index.wxss**/

.page {
  margin: 0;
  padding: 0;
  line-height: 1.6;
  overflow: hidden;
  }

  .name {
    font-weight: 600;
}

.name {
  font-weight: 600;
  font-size: 20px;
}

.time {
  font-size: 15px;
  font-weight: 400;
  margin-left: 10px;
}

.address-phone {
  height: 90rpx;
  display: flex;
  align-content: center;
  justify-content: space-between;
  border-bottom: 1px #eee solid;
}

.share {
  height: 61px;
  width: 61px;
  background: white;
  border-radius: 50%;
  border: 1px solid rgb(254, 115, 108);
  color: rgb(254, 115, 108);
  display: flex;
  align-items: center;
  justify-content: center;
  box-shadow: 0 0 8px #aaa;
  z-index: 600;
  position: fixed;
  top: 100rpx;
}

.address {
  display: flex;
  flex: 0 0 75%;
  flex-direction: column;
  justify-content: center;
  padding-left: 10%;
  font-size: 15px;
  color: #996;
  font-weight: 400;
  border-right: 1px #eee solid;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-position: 5%;
}

.phone {
  display: flex;
  flex: 0 0 20%;
  height: 100%;
  background-image: url("https://yzt.wangjiyu.cn/wechat/src/phone.svg");
  background-repeat: no-repeat;
  background-position: 30%;
}

.items {
  height: 230rpx;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  border-bottom: 1px #eee solid;
  margin-bottom: 5px;
}

.items-items {
  display: flex;
  flex: 0 0 20%;
  flex-wrap: wrap;
  height: 100%;
  align-items: center;
  justify-content: center;
  font-size: 15px;
}

.items-img {
  height: 70px;
  width: 70px;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 10px;
}

.items-items image {
  width: 36px;
  height: 36px;
}
.items-items-word{
  font-size: 13px;
  margin-top: -10px;
}

.first image {
  height: 100%;
  width: 100%;
  border-radius: 50%;
}

.second {
  background-color: #fc6;
}

.third {
  background-color: #f96;
}

.forth {
  background-color: #6cf;
}

.fifth image{  
  height: 100%;
  width: 100%;
  border-radius: 50%;
}

.demo-row {
  height: 100%;
}

.thetabs {
  background-color: white;
}

.swiper {
  height: 400rpx;
  width: 100%;
}

.swiper image {
  height: 100%;
  width: 100%;
}

.swiper-word {
  height: 70rpx;
  width: 100%;
}

.swiper p {
  height: 100%;
  width: 100%;
}

.slide-word {
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-position: 3%;
  padding-left: 30px;
}

.row-class {
  background: #fff;
}

.col-class {
  height: 32px;
  line-height: 32px;
  color: #fff;
  text-align: center;
  background: #2d8cf0;
  font-size: 12px;
}

.col-class.light {
  background: #5cadff;
}

